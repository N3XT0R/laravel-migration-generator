{
  "name": "n3xt0r/laravel-migration-generator",
  "description": "create migrations from database",
  "type": "library",
  "keywords": [
    "laravel",
    "migrations",
    "migration-generator",
    "referential integrity"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-pdo_mysql": "*",
    "doctrine/dbal": "^2.10",
    "illuminate/database": "^5.6||^6.0",
    "illuminate/view": "^5.6|^6.0",
    "illuminate/support": "^5.6|^6.0",
    "illuminate/container": "^5.6|^6.0",
    "marcj/topsort": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "N3XT0R\\MigrationGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Beliaev",
      "email": "info@php-dev.info"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "N3XT0R\\MigrationGenerator\\Providers\\MigrationGeneratorServiceProvider"
      ]
    }
  }
}
