{
  "name": "n3xt0r/laravel-migration-generator",
  "description": "create migrations from database",
  "type": "library",
  "require": {
    "php": ">=7.2.0",
    "doctrine/dbal": "^2.10",
    "illuminate/database": "^5.6",
    "illuminate/support": "^5.6",
    "marcj/topsort": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "N3XT0R\\MigrationGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Beliaev",
      "email": "info@php-dev.info"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "N3XT0R\\MigrationGenerator\\MigrationGeneratorServiceProvider"
      ]
    }
  }
}
